{
  "name": "new-es6",
  "version": "0.0.0",
  "description": "",
  "main": "dist/bundle.js",
  "scripts": {
    "incv": "npm version patch -m'Version incremented to %s'",
    "coa": "git add -A && git commit -m'fast commit'",
    "push": "git push --follow-tags",
    "gitoutahere": "npm run coa && npm run incv && npm run push",
    "build": "browserify --debug -t [ babelify --presets [ es2015 ] ] src/index.js -o bundle.js",
    "watch": "watchify --debug -t [ babelify --presets [ es2015 ] ] src/index.js -o bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/brokenalarms/new-es6.git"
  },
