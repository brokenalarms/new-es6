  "bugs": {
    "url": "https://github.com/brokenalarms/new-es6/issues"
  },
  "homepage": "https://github.com/brokenalarms/new-es6#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "devtool": "^1.9.1",
    "i": "^0.3.4",
    "live-server": "^0.9.2",
    "watchify": "^3.7.0"
  }
}
